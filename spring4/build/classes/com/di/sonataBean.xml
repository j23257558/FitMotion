<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
   <bean id="myCar" class="com.di.Sonata">
   		<constructor-arg index="0" type="java.lang.String" value="소라색"/>
   		<constructor-arg index="1" type="int" value="100"/>
   		<constructor-arg index="2" type="int" value="4"/>
   		<constructor-arg index="3" type="java.lang.String" value="미니쿠퍼"/>
   	</bean>
   <bean id="himCar" class="com.di.Sonata">
   		<constructor-arg index="0" type="java.lang.String" value="희색"/>
   		<constructor-arg index="1" type="int" value="50"/>
   		<constructor-arg index="2" type="int" value="4"/>
   		<constructor-arg index="3" type="java.lang.String" value="벤츠"/>
   </bean>
    <bean id="herCar" class="com.di.Sonata">
   		<constructor-arg index="0" type="java.lang.String" value="핑크색"/>
   		<constructor-arg index="1" type="int" value="50"/>
   		<constructor-arg index="2" type="int" value="4"/>
   		<constructor-arg index="3" type="java.lang.String" value="모하비"/>
   </bean>
</beans>